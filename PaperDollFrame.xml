				<Scripts>
					<OnLoad>
						PaperDollItemSlotButton_OnLoad();
					</OnLoad>
					<OnEvent>
						if ( event == "MERCHANT_UPDATE" ) then
							-- update ammo amount shown in frame after selling ammo to a vendor
							PaperDollItemSlotButton_Update();
							return;
						end
						if ( event == "PLAYERBANKSLOTS_CHANGED" ) then
							-- update ammo amount shown in frame after moving ammo to the bank
							PaperDollItemSlotButton_Update();
							return;
						end
						PaperDollItemSlotButton_OnEvent(self, event, ...);
					</OnEvent>
					<OnShow>
						PaperDollItemSlotButton_OnShow();

						this:RegisterEvent("MERCHANT_UPDATE");
						this:RegisterEvent("PLAYERBANKSLOTS_CHANGED");
					</OnShow>
					<OnHide>
						PaperDollItemSlotButton_OnHide();

						this:UnregisterEvent("MERCHANT_UPDATE");
						this:UnregisterEvent("PLAYERBANKSLOTS_CHANGED");
					</OnHide>
					<OnClick>
						PaperDollItemSlotButton_OnClick(arg1);
					</OnClick>
					<OnDragStart>
						PaperDollItemSlotButton_OnClick("LeftButton");
					</OnDragStart>
					<OnReceiveDrag>
						PaperDollItemSlotButton_OnClick("LeftButton");
					</OnReceiveDrag>
					<OnEnter>
						PaperDollItemSlotButton_OnEnter(self);
					</OnEnter>
					<OnLeave>
						GameTooltip:Hide();
						ResetCursor();
					</OnLeave>
